{
    "manifest_version": 3,
    "name": "Our Env",
    "version": "0.0.1",
    "description": "A Chrome extension to developers & students for the productivity",
    "chrome_url_overrides": {
        "newtab": "pop.html"
    },
    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "js": [
                "app.js"
            ],
            "css": [
                "app.css"
            ],
            "matches": [
                "<all_urls>"
            ],
            "permissions": [
                "storage",
                "tabs",
                "activeTab"
            ]
        }
    ],
    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "offscreen",
        "tabs"
    ]
}